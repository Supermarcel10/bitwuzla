(declare-const x4 Bool)
(set-option :prop-normalize true)
(set-option :bv-solver "prop")
(declare-const x1 Bool)
(declare-const x2 Bool)
(declare-const x3 Bool)
(declare-const x Bool)
(set-info :status sat)
(check-sat-assuming ((not (= (and x2 x1) (= x3 x)))))
(set-info :status sat)
(check-sat-assuming ((not (= x3 x)) (or x3 x4)))
(set-info :status sat)
(check-sat-assuming ((ite (= (and x2 x1) (= x3 x)) false true)))
